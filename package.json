{
  "name": "redux-thunk-promise",
  "version": "0.0.0-development",
  "description": "Thunk and promise middleware for Redux",
  "main": "src/index.ts",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "NODE_ENV=test mocha -r ts-node/register test/**/*.test.ts",
    "coveralls": "NODE_ENV=test nyc npm test | coveralls",
    "build": "tsc",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanovazquez/redux-thunk-promise.git"
  },
  "keywords": [
    "redux",
    "thunk",
    "promise",
    "middleware",
    "redux-middleware"
  ],
  "author": "nanovazquez",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/nanovazquez/redux-thunk-promise/issues"
  },
  "homepage": "https://github.com/nanovazquez/redux-thunk-promise#readme",
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "typings",
      "src/**/index.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "text-lcov"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/sinon": "^4.1.3",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.1",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "semantic-release": "^15.0.0",
    "sinon": "^4.4.2",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
}
